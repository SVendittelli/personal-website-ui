{
  "ci": {
    "collect": {
      "startServerCommand": "docker-compose up --build",
      "url": [
        "http://localhost/",
        "http://localhost/experience"
      ]
    },
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "uses-http2": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
